                    {
                        "data": {
                            "id": "cl7rjp2i3q80",
                            "created": 1657028406502,
                            "text": "产品属性",
                            "layout": null,
                            "expandState": "collapse"
                        },
                        "children": [
                            {
                                "data": {
                                    "id": "cl7rjr0py4o0",
                                    "created": 1657028410748,
                                    "text": "1.产品名称\n2.产品设计\n3.产品包装\n4.产品标签",
                                    "layout": null
                                },
                                "children": []
                            }
                        ]
                    },
                    {
                        "data": {
                            "id": "cl7rkdivesg0",
                            "created": 1657028459735,
                            "text": "营销支持",
                            "layout": null,
                            "expandState": "collapse"
                        },
                        "children": [
                            {
                                "data": {
                                    "id": "cl7rkf74z8g0",
                                    "created": 1657028463379,
                                    "text": "1.交货\n2.保证\n3.安装\n4.售后",
                                    "layout": null
                                },
                                "children": []
                            }
